/* Module 1 digital pins */
#define CS_DI1 10
#define DC_DI1 9
#define RST_DI1 8

/* Module 2 digital pins */
#define CS_DI2 7
#define DC_DI2 6 
#define RST_DI2 5

/* Optional module 3 digital pins (uncomment to use) */
#define CS_DI3 4 
#define DC_DI3 3 
#define RST_DI3 2

#define EYEOPEN 0
#define EYECLOSED 1

/* Create an instance of the library for each display (uncomment all lines below if you are using 3 modules) */
HCuOLED HCuOLED1(SSD1307, CS_DI1, DC_DI1, RST_DI1); // For SH1106 displays (HCMODU0058 & HCMODU0059)
HCuOLED HCuOLED2(SSD1307, CS_DI2, DC_DI2, RST_DI2); // For SH1106 displays (HCMODU0058 & HCMODU0059)
HCuOLED HCuOLED3(SH1106, CS_DI3, DC_DI3, RST_DI3); // For SSD1307 displays (HCMODU0050 & HCMODU0052)

void setup() 
{
  /* Reset the display */
  //delay(1000);
  
int LAS = 0;
int las = 0;
  
  HCuOLED1.Reset();
  HCuOLED2.Reset();
  HCuOLED3.Reset(); /* Uncomment for 3 modules */
}

void loop() 
{


 if((digitalRead(A2) == HIGH))
 {
  /* Display the face with eyes open */
  HCuOLED1.ClearBuffer();
  HCuOLED1.Cursor(24,35);
  HCuOLED3.Cursor(0,2);
  HCuOLED1.Bitmap(128, 8, Eye_Left[EYEOPEN]); 
  HCuOLED1.Refresh();
  
  HCuOLED2.ClearBuffer(); 
  HCuOLED2.Cursor(24,25);
  HCuOLED2.Cursor(0,3);
  HCuOLED2.Bitmap(128, 8, Tiny_Logo_Resistor); 
  
  HCuOLED3.Cursor(0,5);
  HCuOLED2.Refresh(); 
  
  HCuOLED3.ClearBuffer(); 
  HCuOLED3.Bitmap(128, 8, Eye_Right[EYEOPEN]); 
  HCuOLED3.Refresh();
 }
 
                 else 
                 
                 {
                     HCuOLED1.ClearBuffer(); 
                     HCuOLED1.Cursor(0,3);
                  HCuOLED1.Bitmap(128, 8, MainL); 
                  HCuOLED1.Refresh();
                  
                    HCuOLED2.ClearBuffer();
                   HCuOLED2.Cursor(0,3); 
                  HCuOLED2.Bitmap(128, 8, MainM); 
                  HCuOLED2.Refresh();
                  
                    HCuOLED3.ClearBuffer(); 
                    HCuOLED3.Cursor(0,3);
                  HCuOLED3.Bitmap(128, 8, MainR); 
                  HCuOLED3.Refresh();
                   
                 }
 
 if((digitalRead(A3) == HIGH))
    {
      
                       

      
        delay(250);
           if((digitalRead(A3) == HIGH))
          {
           while((digitalRead(A3)) == HIGH)
                    {
                pinMode(A1, OUTPUT); // LASAR
                digitalWrite(A1,HIGH);
    
                HCuOLED3.ClearBuffer();
                HCuOLED3.Cursor(0,0); 
                HCuOLED3.Bitmap(128, 8, Eye_Right[EYEOPEN]); 
                HCuOLED3.Cursor(35,32);
                HCuOLED3.SetFont(LCDLarge_24pt);
                HCuOLED3.Print("LASAR!");
                HCuOLED3.SetFont(MedProp_11pt);
                HCuOLED3.Cursor(0,0);
                HCuOLED3.Refresh();
                    } 
          }  else 
                   {
                       digitalWrite(A1,LOW);
                       HCuOLED3.ClearBuffer();
                       HCuOLED3.Cursor(0,0); 
                       HCuOLED3.Bitmap(128, 8, Eye_Right[EYEOPEN]); 
                       HCuOLED3.Cursor(0,0); 
                       HCuOLED3.Refresh();
                      
                   }
          
 
                    
    }
    

