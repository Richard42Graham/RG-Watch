#define WREN  6

void fmWriteEnable() {
  digitalWrite(MEM_CS, LOW);
  SPI.transfer(WREN);
  digitalWrite(MEM_CS, HIGH);
}

void setup() {

  delay(1000);

  Serial.begin(9600);
  Serial.println("Startup...");

  pinMode(MEM_CS, OUTPUT);
  digitalWrite(MEM_CS, HIGH);

  SPI.setDataMode(SPI_MODE0);
  SPI.setClockDivider(SPI_CLOCK_DIV4);
  SPI.begin();

}

